@model DatabaseConnectie.Models.User

@{
    ViewBag.Title = "User Details";
}

<h2>User Details</h2>

<div>
    <dl class="dl-horizontal">
        <dt>User ID:</dt>
        <dd>@Model.user_id</dd>
        <dt>Username:</dt>
        <dd>@Model.username</dd>
        <dt>Email:</dt>
        <dd>@Model.email</dd>
    </dl>
</div>
